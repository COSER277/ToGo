/**app.wxss**/
@import './styles/iconfont.wxss';
/* 初始化样式 */
page,view,text,swiper,swiper-item,image,navigator {
  padding:0;
  margin:0;
  box-sizing:border-box;
}

page {
  /* --设置主题颜色 定义变量 */
  --themeColor:#1296db;
  /* 字体大小 
  1px=2rpx
  14px=28rpx
  */
  font-size:28rpx;
}
image{
  width: 100%;
}